
; Fibonacci XASM
; 24/03/2015

lda $r0, @str_enter
push $r0
insc Print

insc InputInt
mov $r4, $ar

lda $r5, @str_null
lda $r6, @str_fib
lda $r7, @str_eq

push $r5
insc PrintLn

mov $r3, 0
loop:
	cmp $r4, $r3
	je .end
	
	push $r4
	call fib
	mov $r0, $ar
	
	push $r6
	insc Print
	push $r4
	insc PrintInt
	push $r7
	insc Print
	push $r0
	insc PrintInt
	push $r5
	insc PrintLn
	
	dec $r4
	jmp loop
.end:
stop

; f_1 = f_2 = 1
; f_n = f_n-1 + f_n-2
fib:
	ldw $r0, ($lb) -4
	mov $r1, 2
	
	cmp $r0, $r1
	jle .end
		dec $r0
		
		push $r0
		call fib
		mov $r0, $ar
		
		push $r0
		
		ldw $r0, ($lb) -4
		sub $r0, $r0, 2
		
		push $r0
		call fib
		mov $r0, $ar
		
		pop $r1
		
		add $r0, $r0, $r1
		
		mov $ar, $r0
		ret 1
	.end:
	mov $ar, 1
	ret 1


str_enter: .ascii "enter fibonacci number: "
str_null: .ascii ""
str_fib: .ascii "fib("
str_eq: .ascii ") = "

