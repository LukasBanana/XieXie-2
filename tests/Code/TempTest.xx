	static void printLine(int n) {
		bool neg
		if n < 0 {
			n := -n
			neg := true
		}
		while n > 0 {
			Console.write(n > 10 ? " " : (neg ? "=" : "-"))
			n--
		}
		Console.writeLine("")
	}
	
