class Base {
	
	int[] f(int x, int y, int z) {
		return { x, y, z }
	}
	
	float f(int x) {
		return 0.0
	}
	
	float f(float x) {
		return x/2.0
	}
	
	float[] g(float x) {
		return { x }
	}
	
}

class Widget {
    /*int f() {
        return 1
    }*/
    int f(int x := 0) {
        return x*2
    }
    float f(float x := 0.0) {
        return x*3.0
    }
    void caller() {
        int   a := f(2)    // a is 1
        int   b := f(1)   // b is 2
        float c := f(2.0) // c is 6.0
    }
}

class ProcTest : Base {
	
