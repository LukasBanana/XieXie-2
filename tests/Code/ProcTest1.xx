class Base {
	
	int[] f(int x, int y, int z) {
		return { x, y, z }
	}
	
	float f(int x) {
		return 0.0
	}
	
	float f(float x) {
		return x/2.0
	}
	
	float[] g(float x) {
		return { x }
	}
	
	int x // base member
	
}

