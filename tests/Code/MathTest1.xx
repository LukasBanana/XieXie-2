extern class Math {
	
	static float Sqrt(float x)
	
	static float Sq(float x)
	
	static float DistanceSq(Vector3 a, Vector3 b)
	
}

class Vector3 {
	
	void Normalize() {
		float len := x*x + y*y + z*z
		
		if len != 0.0 and len != 1.0 {
			len := 1.0 / Math.Sqrt(len)
			
			x *= len
			y *= len
			z *= len
		}
	}
	
	float x, y, z
	
}

class Test {
	
	int Fibonacci(int n) {
		if n <= 2 {
			return 1
		}
		return Fibonacci(n - 1) + Fibonacci(n - 2)
	}
	
