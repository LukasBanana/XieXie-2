class Vector3 {
	
	init() {
		x,y,z := 0.0
	}
	
	void normalize() {
		float len := x*x + y*y + z*z
		
		if len != 0.0 and len != 1.0 {
			len := 1.0 / Math.sqrt(len)
			
			x *= len
			y *= len
			z *= len
		}
	}
	
	String toString() {
		String s
		for int i := 0 ; i < { x, y, z }.size() ; i++ {
			s += i.toString()
		}
		return s
	}
	
	float x, y, z
	
}

