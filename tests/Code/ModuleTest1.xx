import MsgBox

class Test {
	
	static void main() {
		
		int answer := MsgBox.show("Info", "Hello, World!", MsgBox.CANCEL_TRY_CONTINUE)
		
		if answer = MsgBox.ID_OK {
			MsgBox.show("Info", "Pressed OK")
		} else if answer = MsgBox.ID_CANCEL {
			MsgBox.show("Info", "Pressed CANCEL")
		} else if answer = MsgBox.ID_TRY {
			MsgBox.show("Info", "Pressed TRY")
		} else if answer = MsgBox.ID_CONTINUE {
			MsgBox.show("Info", "Pressed CONTINUE")
		}
		
	}
	
}

