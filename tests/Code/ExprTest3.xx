import Console

/// Test for the 'instance of' expression
class Test {
	
	static void main() {
		
		Object a := "test"
		var b := { 1 }
		
		if a is Console {
			Console.writeLine("'a' is a Console")
		} else if a is String {
			Console.writeLine("'a' is a String")
		} else if a is Test {
			Console.writeLine("'a' is a Test")
		}
		
		if b is int[] {
			Console.writeLine("'b' is a int[]")
		} else if b is float[] {
			Console.writeLine("'b' is a float[]")
		}
		
	}
	
}

