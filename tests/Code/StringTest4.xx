import Console, ASCII

class Test {
	
	static void main() {
		new Test()
	}
	
	void printLn(String s) {
		Console.writeLine(s)
	}
	
	init() {
		
		printLn(ASCII.toLower("Foo Bar"))
		
		var s := Console.readString()
		while s.size() > 0 {
			printLn(s)
			s.resize(s.size() - 1)
		}
		
	}
	
}
